#logger config used by RSS Item Importer
log4j.logger.rssimporter=INFO, stdout, rssimp
log4j.appender.rssimp=org.apache.log4j.RollingFileAppender
log4j.appender.rssimp.File=RSS_Import_Support/PSORSSItemImporter.log
log4j.appender.rssimp.MaxFileSize=10MB
log4j.appender.rssimp.MaxBackupIndex=1
log4j.appender.rssimp.layout=org.apache.log4j.SimpleLayout